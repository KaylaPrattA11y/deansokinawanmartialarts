---
import Layout from "../../layouts/Layout.astro";
import { BASE_URL, ROUTES, SITE_SEO } from "../../config/site";

const pageDesc = `Latest news and updates from our dojo.`;
const redirectTarget = `${BASE_URL}${ROUTES.news}1/`;
---

<Layout title={`News & Updates | ${SITE_SEO.name.long}`} description={pageDesc}>
  <script is:inline define:vars={{ redirectTarget }}>
    window.location.replace(redirectTarget);
  </script>
</Layout>